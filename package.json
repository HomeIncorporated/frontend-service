{
  "name": "angular-nimble",
  "version": "1.0.0",
  "description": "NIMBLE front-end",
  "scripts": {
    "b": "cross-env NODE_ENV=development webpack --mode development",
    "p": "cross-env NODE_ENV=production webpack --mode production",
    "s": "webpack-dev-server --inline --hot --progress --port 8090 --display-modules",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "i18n": "./node_modules/.bin/ng-xi18n -p ./src/tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "~7.2",
    "@angular/common": "~7.2",
    "@angular/compiler": "~7.2",
    "@angular/compiler-cli": "~7.2",
    "@angular/core": "~7.2",
    "@angular/forms": "~7.2",
    "@angular/http": "~7.2",
    "@angular/platform-browser": "~7.2",
    "@angular/platform-browser-dynamic": "~7.2",
    "@angular/platform-server": "~7.2",
    "@angular/router": "~7.2",
    "@angular/upgrade": "~7.2",
    "@ng-bootstrap/ng-bootstrap": "^1.1.x",
    "@swimlane/ngx-charts": "7.4.0",
    "@swimlane/ngx-graph": "4.3.0",
    "core-js": "~2.6",
    "countries-list": "^2.4.x",
    "d3": "5.0.0",
    "file-saver": "^1.3.8",
    "jshint": "^2.9.x",
    "lunr": "^2.3.0",
    "moment": "^2.22.x",
    "ng2-cookies": "^1.0.12",
    "rxjs": "~6.4",
    "rxjs-compat": "^6.5.2",
    "underscore": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "~7.3",
    "@types/core-js": "~2.5",
    "@types/node": "~10.12",
    "angular-router-loader": "~0.8",
    "angular2-template-loader": "~0.6",
    "awesome-typescript-loader": "~5.2",
    "babel-core": "~6.26",
    "babel-loader": "~7.1",
    "babel-preset-env": "~1.7",
    "clean-webpack-plugin": "~0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "~5.2",
    "css-loader": "~1.0",
    "cssnano": "~4.1",
    "del-cli": "~1.1",
    "file-loader": "^4.0.0",
    "html-loader": "~0.5",
    "html-webpack-plugin": "~3.2",
    "lite-server": "~2.4",
    "node-sass": "~4.12",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "sass-loader": "~7.1",
    "style-loader": "~0.23",
    "to-string-loader": "~1.1",
    "typescript": "~3.1",
    "uglifyjs-webpack-plugin": "~2.0",
    "webpack": "~4.29",
    "webpack-cli": "~3.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "~4.2"
  },
  "repository": {}
}
